import java.util.Scanner;
public class Lab02_3 {

	public static void main(String[] args) {
		
		Scanner in= new Scanner(System.in);
		
		System.out.print("올해 1월 1일은 어떤요일인가요? ");
		String weekday[]= {"일요일","월요일","화요일","수요일","목요일","금요일","토요일"};
		int days[]= {0,31,28,31,30,31,30,31,31,30,31,30,31};
		
		String day=in.next();
		int num=0;
		
		for(int i=0;i<7;i++)
			if(day.equals(weekday[i]))
				num=i;
		
		
		System.out.println("1월");
		for(int i=0;i<num;i++)
		{
			System.out.print("* ");
		}
		for(int i=1;i<=31;i++)
		{
			System.out.print(i+" ");
			if((i+num)%7==0)
				System.out.println();
		}
		System.out.println();
		
		System.out.println("2월");
		for(int i=0;i<num;i++)
		{
			for(int k=0;k<days[1]%7+num;k++)
				System.out.print("* ");
		}
		for(int i=1;i<=31;i++)
		{
			System.out.print(i+" ");
			if((i+days[1]%7+num)%7==0)
				System.out.println();
		}
		System.out.println();
		
		
		
		for(int i=3;i<days.length;i++)
		{
			System.out.println(i+"월");
			
			for(int k=0;k<days[i-1]%7;k++)
				System.out.print("* ");
			
			
			for(int j=1;j<=days[i];j++)
			{
				
				System.out.print(j+" ");
				if((j+days[i-1]%7)%7==0)
					System.out.println();
			}
			System.out.println();
		}
		
	}

}
